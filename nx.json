{
  "npmScope": "bison",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "front": { "tags": [] },
    "front-e2e": { "tags": [], "implicitDependencies": ["front"] },
    "shared-ui": { "tags": [] },
    "shared-ui-e2e": { "tags": [], "implicitDependencies": ["shared-ui"] },
    "backend-graphql-api": { "tags": [] }
  }
}
